{
  "name": "@dargmuesli/nuxt-vio",
  "version": "1.4.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "18"
  },
  "packageManager": "pnpm@8.2.0",
  "files": [
    "components",
    "composables",
    "layouts",
    "server",
    "plugins",
    "utils",
    "nuxt.config.ts"
  ],
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi prepare & nuxi dev .playground",
    "build": "nuxi build .playground",
    "generate": "nuxi generate .playground",
    "preview": "nuxi preview .playground",
    "prepare": "pnpm husky install && pnpm nuxt prepare",
    "lint": "eslint --cache --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:staged": "pnpm lint-staged"
  },
  "dependencies": {
    "@dargmuesli/nuxt-cookie-control": "5.4.0",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@nuxtjs/tailwindcss": "6.6.5",
    "@tailwindcss/typography": "0.5.9",
    "vue-gtag": "2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@intlify/eslint-plugin-vue-i18n": "2.0.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-yml": "1.5.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "nuxt": "3.4.0",
    "prettier": "2.8.7",
    "typescript": "5.0.4"
  }
}
