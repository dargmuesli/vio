{
  "name": "@dargmuesli/nuxt-vio",
  "version": "1.7.1",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "18"
  },
  "packageManager": "pnpm@8.2.0",
  "files": [
    "assets",
    "components",
    "composables",
    "layouts",
    "server",
    "pages",
    "plugins",
    "utils",
    "error.vue",
    "nuxt.config.ts",
    "tailwind.config.ts"
  ],
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi dev .playground",
    "build": "nuxi build .playground",
    "generate": "nuxi generate .playground",
    "preview": "nuxi preview .playground",
    "prepare": "pnpm husky install && pnpm nuxt prepare .playground",
    "lint": "pnpm lint:js && pnpm lint:ts && pnpm lint:style",
    "lint:js": "eslint --cache --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:staged": "pnpm lint-staged",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:ts": "pnpm nuxt typecheck"
  },
  "dependencies": {
    "@dargmuesli/nuxt-cookie-control": "5.6.0",
    "@http-util/status-i18n": "0.6.2",
    "@nuxtjs/html-validator": "1.2.4",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@nuxtjs/tailwindcss": "6.6.6",
    "@tailwindcss/typography": "0.5.9",
    "nuxt-seo-kit": "1.3.6",
    "vue-gtag": "2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@intlify/eslint-plugin-vue-i18n": "2.0.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-yml": "1.5.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "nuxt": "3.4.1",
    "prettier": "2.8.7",
    "stylelint": "15.4.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "32.0.0",
    "stylelint-no-unsupported-browser-features": "6.1.0",
    "typescript": "5.0.4",
    "vue-tsc": "1.2.0"
  }
}
